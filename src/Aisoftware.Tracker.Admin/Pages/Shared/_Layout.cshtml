            </div>
            <div class="page-container">
                @RenderBody()
                <footer class="footer">
                    <div class="footer-content">
                        <p class="m-b-0">Copyright © 2020 Aisoftware. Todos os direitos reservados.</p>
                    </div>
                </footer>
            </div>
        </div>
